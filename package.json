{
  "name": "mome-web-app",
  "version": "1.0.0",
  "description": "Express And MySQL Application",
  "scripts": {
    "start:dev": "nodemon --exec babel-node -- src/index.js",
    "start": "babel-node -- src/index.js",
    "clean": "rimraf node_modules && rimraf dist && npm install",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "lint": "eslint client server; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bikranshu/mome-web-app.git"
  },
  "keywords": [
    "javascript",
    "node",
    "es6",
    "express",
    "webpack"
  ],
  "author": "Krishna Timilsina <bikranshu.t@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.2.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.7",
    "helmet": "^4.2.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "^0.21.13",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "path": "^0.12.7",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^4.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/register": "^7.12.10",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "husky": "^4.3.6",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "12.16.3",
    "npm": ">= 3.10.8"
  }
}
